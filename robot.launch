<launch>
<!-- DEBUT NXT -->
	<!-- nxt_ros launch -->
	<node pkg="nxt_ros" type="nxt_ros.py" name="nxt_ros" output="screen" respawn="true">
		<rosparam command="load" file="$(find lunarNXT)/robot.yaml" />
	</node> 

	<!-- lunarNXT launch -->
	<node pkg="lunarNXT" type="launcher" name="main_node" output="screen" respawn="true" />
<!-- FIN NXT -->

</launch>
